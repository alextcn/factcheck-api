    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "strictNullChecks": true
  },
  "lib": ["es2015"],
  "include": ["**/*.ts", "**/*.tsx", "scripts/**/*.js"],
  "exclude": ["node_modules"]
}